let arr = [1,2,3,4,5];

let max = Math.max(...arr);
let min = Math.min(...arr);

console.log("Nilai Maximum = " +max);
console.log("Nilai Minimum = " +min);

let total = 0;

for (let i=0; i<arr.length; i++) {
  if(arr[i] !== 2) {
    total += arr[i];
  }
}

console.log("Jumlahkan Bilangan Kecuali Angka 3 = " +total);