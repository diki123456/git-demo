Pme(number) {
    // Deklarasi variable untuk menyimpan hasil
 program untuk mengecek apakah sebuah bilangan prima atau bukan

function checkPri   let result;

    // Periksa apakah bilangan tersebut habis dibagi oleh angka 2
    if (number % 2 === 0) {
        result = false;
    } else {
        // Periksa apakah bilangan tersebut habis dibagi oleh angka 3
        if (number % 3 === 0) {
            result = false;
        } else {
            // Periksa apakah bilangan tersebut habis dibagi oleh angka 5
            if (number % 5 === 0) {
                result = false;
            } else {
                // Periksa apakah bilangan tersebut habis dibagi oleh angka 7
                if (number % 7 === 0) {
                    result = false;
                } else {
                    // Jika tidak habis dibagi oleh angka diatas, maka bilangan tersebut adalah prima
                    result = true;
                }
            }
        }
    }
    // Keluarkan hasil
    console.log(result);
}

// Contoh
checkPrime(17); // Output: true